{
  "name": "robot/parser",
  "description": "Robot Parser",
  "type": "project",
  "license": "MIT",
  "require": {
      "php": "^8.1",
      "phpunit/phpunit": "^9.5"
  },
  "autoload": {
      "psr-4": {
          "App\\": "app/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "scripts": {
      "test": "phpunit"
  },
  "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
